{
  "name": "rasid/activity",
  "description": "Monitor user activity easily!",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Osama Roshdi",
      "email": "osamaaroshdy@gmail.com"
    }
  ],
  "minimum-stability": "",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "Rasid\\Activity\\": "src"
    }
